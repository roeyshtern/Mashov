<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/home_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.mashov.HomeActivity">


    <android.support.design.widget.BottomNavigationView
        android:id="@+id/hl_navigation_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        app:itemBackground="@android:color"
        app:itemIconTint="@drawable/navigation_bar_color_state"
        app:itemTextColor="@drawable/navigation_bar_color_state"
        app:menu="@menu/bottom_navigation_bar_home" />

    <FrameLayout
        android:id="@+id/hl_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#f1f1f1"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

    </FrameLayout>


</RelativeLayout>
